<!DOCTYPE html>
<html lang="@Orchard.CultureName()">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@RenderTitleSegments(Site.SiteName, "before")</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.0/css/bulma.min.css">
    @*<script src="~/Theme.Arcstone/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/Theme.Arcstone/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/Theme.Arcstone/lib/jquery-validation/dist/jquery.validate.min.js"></script>
        <script src="~/Theme.Arcstone/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>*@
    <span style="display:none">this is from my module</span>
    <!-- Bootstrap core JavaScript -->
    <script asp-name="DataTableJs" at="Head" depends-on="jQuery"></script>
    <script asp-name="jQuery" at="Head"></script>
    <script asp-name="tableInitJs" at="Foot" defer></script>
    <style asp-name="DataTableCss" at="Head"></style>
    <style asp-name="BulmaCss" at="Head"></style>
    <resources type="Meta" />
